pipeline {
    agent any
    stages {
        stage ('stage 1'){
            steps {
                echo 'Hello World!'
        
            }
        }
        stage ('stage 2'){
        steps {
            echo 'helloworld'
    }
}
        stage ('stage 3'){
            steps {
                echo 'hello world 3'
            }
        }
        stage ('stage 4'){
            steps {
                echo 'hello world 4'
            }
        }
    }
}    
